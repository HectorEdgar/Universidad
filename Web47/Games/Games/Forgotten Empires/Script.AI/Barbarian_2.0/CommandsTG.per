; 230 - Mush
; 33 - Boom
; 32 - Rush

(defrule
	(game-time > 600)
=>
	(up-jump-rule 3)
)

(defrule
	(game-time > 30)
	(game-time <= 600)
	(goal gl-current-age gv-dark)
	(up-compare-const gv-michi-style != 1)
	(taunt-detected any-human-ally 230)
=>
	(acknowledge-taunt every-ally 230)
	(set-goal gl-strategy-aggressiveness-level VERY-LOW)
	(set-goal gl-strategy fi-monk-rush)
	(set-goal gl-strategy-type FC2)
	(chat-to-allies "I'll go monks.")
	(set-strategic-number sn-target-evaluation-hitpoints 1)
)

(defrule
	(game-time > 30)
	(game-time <= 600)
	(goal gl-current-age gv-dark)
